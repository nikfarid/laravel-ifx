{
  "name": "poyii/laravel-ifx",
  "description": "Informix DB driver for Laravel 5",
  "keywords": ["informix", "laravel", "laravel 5", "ifx" ],
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "llaiayumi",
      "email": "872833541@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.2.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0|8.*",
    "illuminate/database": "5.2.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0|8.*",
    "illuminate/pagination": "5.2.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0|8.*",
    "illuminate/encryption": "5.2.*|5.5.*|5.6.*|5.7.*|5.8.*|6.0|8.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~4.0|^6.5|^7.0|^8.0",
    "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*",
    "mockery/mockery": "~0.9.4|^1.1"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Poyii\\Informix\\InformixDBServiceProvider"
        ]
    }
  },
  "autoload": {
    "psr-4": {
      "Poyii\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
